/* pages/work/work.wxss */
#body {
	/* background-color: #fff; */
}
.display_none{
	height: 0rpx!important;
	opacity: 0!important;
}

.tab-flex {
	display: flex;
	justify-content: space-between;
	height: 70rpx;
	line-height: 70rpx;
	box-shadow: 0px 4rpx 40rpx 0px rgba(102, 102, 102, 0.12);
	font-size: 36rpx;
	font-weight: 600;
}

.tab-item {
	flex: 0 0 50%;
	text-align: center;

}

.tab-active::after {
	content: "";
	width: 200rpx;
	height: 25rpx;
	position: absolute;
	margin-left: -150rpx;
	margin-top: 35rpx;
	background: linear-gradient(270deg, rgba(255, 242, 158, 0.3) 0%, #FFE065 100%);
	z-index: -1;
}

/* 选择课程的样式 */
.F-box {
	width: 560rpx;
	max-height: 600rpx;
	border-radius: 20rpx;
	margin: 100rpx auto 0rpx auto;
	text-align: center;
	background-color: #fff;
	box-shadow: 0px 4rpx 40rpx 0px rgba(102, 102, 102, 0.12);
	position: relative;
	z-index: 99999;
}

.F-box-active {
	width: 700rpx !important;
	margin: 10rpx auto!important ;
}


.F-input {
	width: 100%;

}

.F-box-none .F-input::before {
	content: "";
	position: absolute;
	width: 160rpx;
	height: 160rpx;
	margin-top: -100rpx;
	margin-left: -80rpx;
	background-size: 100%;
	z-index: -1;
	background-repeat: no-repeat;
	background-image: url('https://admin.cradlela-art.com/static/_cradle_/icon/default.png');
}

.F-selected {
	font-weight: 700;
	padding-left: 50rpx;
	height: 100%;
	text-align: left;
	line-height: 120rpx;
}

.F-selected::after {
	content: "";
	float: right;
	width: 60rpx;
	height: 60rpx;
	background-size: 100%;
	background-repeat: no-repeat;
	background-image: url('https://admin.cradlela-art.com/static/_cradle_/icon/selectBtn.png');
	margin-top: 30rpx;
	margin-right: 10rpx;
}

.F-list {
	width: 560rpx;
	max-height: 600rpx;
	background-color: #fff;
	border-radius: 20rpx;
	box-shadow: 0px 4rpx 40rpx 0px rgba(102, 102, 102, 0.12);
}

.F-box-active .F-list {
	width: 700rpx !important;
}

.F-row {
	width: 510rpx;
	line-height: 80rpx;
	padding-left: 50rpx;
	display: flex;
	justify-content: left;
}

.F-box-active .F-row {
	width: 700rpx;
}

.row-checkbox {
	flex: 0 0 25rpx;
}

.row-title {

	padding-right: 50rpx;
	text-align: left;

	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.F-box-none .F-row .row-title {
	flex: 0 0 300rpx;
}

.F-box-active .F-row .row-title {
	flex: 0 0 400rpx;
}

.row-state {
	flex: 0 0 0rpx;
}

.row-state-0::before {
	content: "  ";
	width: 16rpx;
	height: 16rpx;
	position: absolute;
	margin-top: 32rpx;
	border-radius: 50%;
	background-color: #FA5151;
	line-height: 80rpx;
}

.row-state-0::after {
	content: '未读';
	font-size: 22rpx;
	color: #B3B6CB;
	position: absolute;
	margin-left: 25rpx;
	line-height: 80rpx;
}

.row-state-1::before {
	content: "  ";
	width: 16rpx;
	height: 16rpx;
	position: absolute;
	margin-top: 32rpx;
	border-radius: 50%;
	background-color: #fff;
	line-height: 80rpx;
}

.row-state-1::after {
	content: '已读';
	font-size: 22rpx;
	color: #B3B6CB;
	position: absolute;
	margin-left: 25rpx;
	line-height: 80rpx;
}

.F-btn {
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	color: #fff;

	border-radius: 20rpx;
	margin: 5rpx 0rpx;
	background-color: #07C160;
}


/* 作业部分的样式 */
.W-box {


}

.red-area{
	position: absolute;
	background: rgba(255,0,0,0.4);
}

.scroll-view_H {
	white-space: nowrap;
}

.scroll-view-item_H {
	display: inline-block;
	min-width: 84rpx;
}

.menuItem {
	flex: 1 1 auto;
	padding: 10rpx 20rpx;
	font-size: 26rpx;
	font-weight: 500;
	line-height: 35rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	text-align: center;
	border-radius: 8rpx;
	background-color: rgba(211, 211, 211, 0.15);
	margin-right: 12rpx;
}

.menuItem-active {

	background-color: #FEDF64;
}

.W-list {
	margin: 0 auto;
	padding-bottom: 102rpx;
}

.W-row {
	width: 650rpx;
	padding: 24rpx;

	border-radius: 20rpx;
	background-color: #fff;
	border: 1px solid #efefef;
	position: relative;
}
.W-row-active .W-row{
	width: 520rpx;
}
.W-chakan{
	width: 100rpx;
	height:100%;
	margin:0rpx 20rpx;
	text-align: center;
	line-height: 130rpx;
	background-color: #fff;
	color: #538EFF;
	font-size: 22rpx;
	border-radius: 16rpx;
	border: 1px solid #efefef;
}

.W-row-active .W-chakan,.W-row-active .W-row{
	box-shadow: 0px 0px 30rpx 0px #ccc;
}

.W-wrap{
	display: flex;
	margin: 20rpx 0rpx;
	border-radius: 20rpx;
	flex-wrap: nowrap;
	justify-content: space-between;
}

.W-row-active .W-line {
	width: 10rpx;
	height: 100%;
	margin-top: -24rpx;
	margin-left: -24rpx;
	border-radius: 20rpx 0 0 20rpx;
	position: absolute;
	background-color: #FEDF64;
}

.W-title {
	width: 520rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	font-size: 24rpx;
	margin-bottom: 8rpx;

}
.W-title-unread::before{
	content:'未读';
	padding:6rpx 10rpx;
	font-size: 16rpx;
	margin: 5rpx 10rpx;
	background-color: #FA5151;
	position:relative;
	border-radius: 8rpx;
	bottom: 3rpx;
	color: #fff;
}

.W-tip-0::before {
	content: '待改正';
	float: right;
	color: #fff;
	font-size: 20rpx;
	padding: 4rpx 10rpx;
	border-radius:0rpx 20rpx 0rpx 20rpx;
	background-color: #FA5151;
	position: absolute;
	right: 0rpx;
	top: -4rpx;
}

.W-tip-1::after {
	content: '已改正';
	float: right;
	color: #fff;
	font-size: 20rpx;
	padding: 4rpx 10rpx;
	border-radius:0rpx 20rpx 0rpx 20rpx;
	background-color: #538EFF;
	position: absolute;
	right: 0rpx;
	top: -4rpx;
}

.W-tip-2::after {
	content: '修改错误';
	float: right;
	color: #fff;
	font-size: 20rpx;
	padding: 4rpx 10rpx;
	border-radius:0rpx 20rpx 0rpx 20rpx;
	background-color: #FFB800;
	position: absolute;
	right: 0rpx;
	top: -4rpx;
}


.W-detail {
	display: -webkit-box;
	overflow: hidden;
	text-overflow: ellipsis;
	word-break: break-all;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	color: #888;
	font-size: 24rpx;

}


#work_btn{
	width: 430rpx;
	height: 80rpx;
	margin: 10rpx 135rpx;

	border: 3rpx solid #538EFF;
	border-radius: 55rpx;
	text-align: center;
	line-height: 80rpx;
	font-weight: 700;
	position: fixed;
	bottom: 0px;
}

#work_img{
	width: 34rpx;
	height: 48rpx;
	vertical-align: text-top;
	margin-right: 20rpx;
}

/* 数据页面 */
.D-box {
	width: 700rpx;
	margin: 25rpx;
	background-color: #fff;
	border-radius: 20rpx;
	box-shadow: 0px 4px 40px 0px rgba(102, 102, 102, 0.12);
	font-size: 26rpx;
}

.D-row {
	display: flex;
	justify-content: flex-start;
	flex-wrap: nowrap;
	padding: 30rpx;
}

.D-title {
	flex: 0 0 150rpx;
	text-align: left;
	overflow: hidden;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.D-progress {
	flex: 0 0 370rpx;
	margin: 0 20rpx;
	display: flex;
}

.D-line {
	height: 40rpx;
	border-radius: 20rpx;
	background: linear-gradient(232deg, #FFF29E 0%, #FFE065 100%);
	text-align: right;
}

.D-lvl-0 {
	width: 1rpx;
	background: linear-gradient(232deg, #FFF29E 0%, #FFE065 100%);
}

.D-lvl-1 {
	width: 100rpx;
}

.D-lvl-2 {
	width: 200rpx;
}

.D-lvl-3 {
	width: 300rpx;
	background: linear-gradient(270deg, #FFD17C 0%, #FF9A00 75%, #FF7500 100%);
}

.D-num {
	margin-left: 10rpx;
}

.D-btn {
	float: 0 0 50rpx;
	font-size: 20rpx;
	color: #B3B6CB;
}

.C-map {
	position: fixed;
	width: 750rpx;
	height: 100vh;
	top: 0px;
	z-index: 99999;
	background-color: rgba(0, 0, 0, 0.5);
}

.C-box {
	width: 650rpx;
	padding: 25rpx;
	margin: 0 auto;
	margin-top: 100rpx;
	background-color: #fff;
	border-radius: 40rpx;
	overflow: hidden;
}

.C-leimu {
	margin-top: 20rpx;
	margin-bottom: 50rpx;
	font-size: 32rpx;
	font-weight: 700;
	position: relative;
	z-index: 1;
}

.C-leimu::before {
	content: "";
	width: 200rpx;
	height: 25rpx;
	margin-left: 0rpx;
	margin-top: 20rpx;
	position: absolute;
	z-index: -1;
	background: linear-gradient(270deg, rgba(255, 242, 158, 0.3) 0%, #FFE065 100%);
}

.C-row {
	margin-bottom: 50rpx;
	font-size: 24rpx;
}

.C-title {
	width: 600rpx;
	overflow: hidden;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.C-title-active {
	width: 600rpx;
}

.C-title::before {
	content: "";
	width: 12rpx;
	height: 12rpx;
	float: right;
	margin-right: 10rpx;

	border-bottom: 2px solid #979797;
	border-right: 2px solid #979797;
	transform: rotate(45deg);
	border-left: 2px solid transparent;
	border-top: 2px solid transparent;
	display: inline-block;


}

.C-progress {
	display: flex;
	margin-top: 15rpx;
}

.C-line {
	height: 24rpx;
	border-radius: 20rpx;
	background: linear-gradient(320deg, #8DB7FF 0%, #5480FF 100%);
	text-align: right;
}

.C-lvl-0 {
	width: 1rpx;
}

.C-lvl-1 {
	width: 100rpx;
}

.C-lvl-2 {
	width: 200rpx;
}

.C-lvl-3 {
	width: 300rpx;
}

.C-num {
	margin-left: 10rpx;
}


